<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DLR_Data_App.Views.CurrentProject.ProjectPage"
             xmlns:vm="clr-namespace:DLR_Data_App.ViewModels.CurrentProject"
             xmlns:localizations="clr-namespace:DLR_Data_App.Localizations;assembly=DLR_Data_App"
             Title="{Binding Title}"
             SelectedTabColor="Blue">

    <TabbedPage.BindingContext>
        <vm:ProjectViewModel />
    </TabbedPage.BindingContext>

    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="{localizations:Translate edit}" Clicked="EditClicked" />
        <ToolbarItem Text="{localizations:Translate save}" Clicked="SaveClicked"/>
    </TabbedPage.ToolbarItems>

    
</TabbedPage>