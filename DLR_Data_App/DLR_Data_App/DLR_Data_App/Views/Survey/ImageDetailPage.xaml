<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DLR_Data_App.Views.Survey.ImageDetailPage"
             xmlns:controls="clr-namespace:DLR_Data_App.Controls"
             xmlns:res="clr-namespace:DLR_Data_App.Localizations"
             Title="{res:Translate survey}">
    <ContentPage.Content>
        <Grid Padding="20">
            <controls:PinchToZoomContainer>
                <controls:PinchToZoomContainer.Content>
                    <Image x:Name="Image" />
                </controls:PinchToZoomContainer.Content>
            </controls:PinchToZoomContainer>
        </Grid>
    </ContentPage.Content>
</ContentPage>